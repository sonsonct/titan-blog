{
  "name": "blog",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "license": "ISC",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:dev": "rimraf dist && nest build",
    "build:staging": "rimraf dist && nest build",
    "build:production": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "rimraf dist && cross-env NODE_ENV=local nest start --watch",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-esm -d ./src/configs/typeorm.ts",
    "migration:generate": "npm run typeorm migration:generate ./src/database/migrations/%npm_config_name%",
    "migration:generate:mac": "npm run typeorm migration:generate ./src/database/migrations/$npm_config_name",
    "migration:create": "npm run typeorm migration:create ./src/database/migrations/%npm_config_name%",
    "migration:revert": "npm run typeorm migration:revert",
    "lint": "eslint . --ext .ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/cloudfront-signer": "^3.541.0",
    "@golevelup/nestjs-rabbitmq": "^3.2.0",
    "@google-cloud/recaptcha-enterprise": "^5.6.0",
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/axios": "^0.1.1",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.0.11",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/terminus": "^9.1.1",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@nestlab/google-recaptcha": "^3.7.0",
    "@types/mime-types": "^2.1.4",
    "aws-sdk": "^2.1379.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "buffer-to-stream": "^1.0.0",
    "cache-manager": "^3.4.4",
    "class-transformer": "0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^2.0.1",
    "compression": "^1.7.4",
    "dayjs": "^1.10.7",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "express-ctx": "^0.1.1",
    "firebase": "^10.8.1",
    "firebase-admin": "^11.11.1",
    "helmet": "^5.1.1",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nest-aws-sdk": "^3.0.1",
    "nestjs-i18n": "^9.1.6",
    "nodemailer": "^6.9.13",
    "numeral": "^2.0.6",
    "openai": "^3.2.1",
    "otp-generator": "^4.0.0",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "remove-accents": "^0.4.4",
    "rxjs": "7.5.6",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^4.5.0",
    "twig": "^1.16.0",
    "typeorm": "^0.3.7",
    "typeorm-cursor-pagination": "^0.9.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.11",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.7",
    "@types/lodash": "^4.14.105",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.14",
    "@types/numeral": "^2.0.2",
    "@types/otp-generator": "^4.0.0",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-watch": "^8.0.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
